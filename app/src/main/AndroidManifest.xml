<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.taan.hasani.moein.volley">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:screenOrientation="portrait"
        android:name="com.taan.hasani.moein.guess_it.appcontroller.AppController"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/MyMaterialTheme2">
        <activity
            android:name="com.taan.hasani.moein.guess_it.game_menu.Loading"
            android:theme="@style/Theme.AppCompat.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.taan.hasani.moein.guess_it.game_menu.Entrance_signup_login" />
        <activity android:name="com.taan.hasani.moein.guess_it.game_menu.Login" />
        <activity android:name="com.taan.hasani.moein.guess_it.game_menu.SignUp" />
        <activity
            android:name="com.taan.hasani.moein.guess_it.game.single_Player"
            android:theme="@style/MyMaterialTheme2" />
        <activity android:name="com.taan.hasani.moein.guess_it.game.choosing_theGame" />
        <activity
            android:name="com.taan.hasani.moein.guess_it.profile.profile"
            android:theme="@style/MyMaterialTheme" />

        <activity android:name="com.taan.hasani.moein.guess_it.game_menu.Main_menu" />
        android:theme="@style/AlertDialog.AppCompat"/>

        <activity android:name="com.taan.hasani.moein.guess_it.profile.change_password" />
        <activity android:name="com.taan.hasani.moein.guess_it.Leader_board.Leader_Board" />
        <activity android:name="com.taan.hasani.moein.guess_it.game.two_player" />
        <activity android:name="com.taan.hasani.moein.guess_it.game.categories_twoPlayer" />
        <activity android:name="com.taan.hasani.moein.guess_it.game.categories_singlePlayer" />
    </application>

</manifest>