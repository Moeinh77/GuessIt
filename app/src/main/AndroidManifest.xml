<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.taan.hasani.moein.volley">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".appcontroller.AppController"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/MyMaterialTheme2">
        <activity
            android:name=".game_menu.Loading"
            android:theme="@style/Theme.AppCompat.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".game_menu.Entrance_signup_login" />
        <activity android:name=".game_menu.Login" />
        <activity android:name=".game_menu.SignUp" />
        <activity android:name=".game.single_Player" />
        <activity android:name=".game.choosing_theGame" />
        <activity
            android:name=".profile.profile"
            android:theme="@style/MyMaterialTheme" />
        <activity android:name=".game_menu.Main_menu" />
        <activity android:name=".profile.change_password"></activity>
    </application>

</manifest>